{
    "name": "raidros/marketplace",
    "type": "library",
    "description": "Marketplace api hub",
    "keywords": [
        "raidros",
        "jub4",
        "marketplace"
    ],
    "homepage": "https://github.com/raidros/marketplace",
    "license": "MIT",
    "authors": [
        {
            "name": "Juliano Bail√£o",
            "email": "julianobailao@gmail.com",
            "homepage": "http://github.com/jub4",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.5|~5.6|~7.0",
        "raidros/collection": "^1.0",
        "raidros/storer": "1.0.*",
        "fzaninotto/faker": "1.6.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Raidros\\Marketplace\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Raidros\\Marketplace\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
